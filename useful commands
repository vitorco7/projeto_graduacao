docker compose -f docker-compose_v3_ubuntu_mysql_local.yaml --profile all up



docker compose exec zabbix-server /bin/bash

docker compose --profile all up
docker system prune -a


entrar no container:
docker exec -it <container_id_or_name> /bin/bash


kill all containers
docker kill $(docker ps -q)

# Check container health statuses
docker ps --format "table {{.Names}}\t{{.Status}}"

# Inspect detailed health status
docker inspect <container_name> | jq '.[].State.Health'
